function start(){
    move();
    buildTower();
    toBottom();
    move();
    buildTower();
    turnRight();
}

function turnRight() {
    turnLeft();
    turnLeft();
    turnLeft();
}

function buildTower() {
    turnLeft();
    putBall();
    move();
    putBall();
    move();
    putBall();
    move();
}

function toBottom() {
    turnRight();
    move();
    turnRight();
    move();
    move();
    move();
    turnLeft();
}
